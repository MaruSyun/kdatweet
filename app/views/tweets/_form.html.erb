<%= form_with(model: @tweet, local: true) do |f| %>
  <div class = "imagef">
    <%= f.label :image do %>
      <%= f.file_field :image %>
    <% end %>
  </div>
  <%= f.text_area :text, placeholder: "text", rows: "10" %>
  <div class = "kda">
    <%= f.number_field :kill, placeholder: "k" %>
    <%= f.number_field :death, placeholder: "d" %>
    <%= f.number_field :assist, placeholder: "a" %>
  </div>
  <%= f.submit "SEND" %>
<% end %>