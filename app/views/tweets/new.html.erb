<div class="contents row">
  <div class="container">
    <%= form_with(model: @tweet, local: true) do |form| %>
      <h3>投稿する</h3>
      <%= form.text_field :kill, placeholder: "kda-k", rows:"1"%>
      <%= form.text_field :death, placeholder: "kda-d", rows:"1"%>
      <%= form.text_field :assist, placeholder: "kda-a", rows:"1"%>
      <%= form.text_field :image, placeholder: "Image Url" %>
      <%= form.text_area :text, placeholder: "text", rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>